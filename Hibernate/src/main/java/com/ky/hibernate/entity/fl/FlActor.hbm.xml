<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 5.3//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">

<hibernate-mapping package="com.ky.hibernate.entity.fl">

	<class name="FlActor" table="FL_ACTOR">
		<id name="actorId" column="ACTOR_ID" >
		    <generator class="sequence"> 
		        <param name="sequence_name">FL_ACTOR_SEQ</param>
		    </generator>
		</id>
		<property name="docType" column="DOC_TYPE" />
		<property name="docId" column="DOC_ID" />
		<property name="roleCode" column="ROLE_CODE" />
		<property name="userId" column="USER_ID" />
		<property name="salutation" />
		<property name="userName" column="USER_NAME" />
		<property name="designation" />
		<property name="actionDate" type="timestamp" column="ACTION_DATE" />
		<property name="grade" />
	</class>

</hibernate-mapping>